# Environment configuration for the Flutter template.
#
# Copy this file to .env before running the app:
#   cp .env.example .env
#
# Or use a pre-configured environment file:
#   cp .env.development .env   (development)
#   cp .env.staging .env       (staging)
#   cp .env.production .env    (production)

# Environment name (development | staging | production)
ENV_NAME=development



# Enable HTTP request/response logging
ENABLE_LOGGING=true

# Show the Flutter debug banner
SHOW_DEBUG_BANNER=true

# Use mock auth datasource (bypasses real API)
USE_MOCK_AUTH=true


# Supabase Configuration

# API base URL
BASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Sentry (Error Tracking - Optional)
SENTRY_DSN=

# Optional: Firebase Cloud Messaging
FCM_SERVER_KEY=



# Google Sign-In (Required for Supabase)
# IMPORTANT: Replace these with YOUR actual OAuth client IDs from Google Cloud Console
# Create 3 OAuth clients: Web (for Supabase), Android, and iOS
# Guide: https://console.cloud.google.com/apis/credentials
GOOGLE_WEB_CLIENT_ID=YOUR_WEB_CLIENT_ID.apps.googleusercontent.com
GOOGLE_ANDROID_CLIENT_ID=YOUR_ANDROID_CLIENT_ID.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=YOUR_IOS_CLIENT_ID.apps.googleusercontent.com
